{
  "name": "timeline",
  "version": "0.1.0",
  "main": "App.tsx",
  "license": "MIT",
  "scripts": {
    "start": "yarn parcel src/index.html",
    "build": "yarn parcel build src/index.html",
    "lint": "yarn lint:ts && yarn lint:es",
    "lint:ts": "yarn tslint src/*.ts{,x} src/**/*.ts{,x} tests/**/*.ts{,x}",
    "lint:es": "yarn eslint . --ext .ts,.tsx",
    "cute": "yarn prettier --single-quote --trailing-comma es5 --write \"{src,tests}/**/*.{ts{,x},html,{s,}css,json}\"",
    "test": "yarn mocha -r ts-node/register tests/**/*.ts{,x}"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "mocha": "^6.2.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "sass": "^1.24.0",
    "sinon": "^7.5.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.12.0"
  }
}
